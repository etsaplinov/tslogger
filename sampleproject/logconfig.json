{
    "Listeners": [
        {
            "Name": "file",
            "Type": "LogFile",
            "Format": "{0}{1}{2}",
            "ListenerConfig": {
                "fileName": "log.txt"
            }
        },
        {
            "name": "console",
            "type": "LogConsole",
            "format": "{0}{2}"
        }
    ],
    "Loggers": [
        {
            "Name": "root",
            "Listeners": [
